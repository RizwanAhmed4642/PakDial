<div class="modal fade text-left show" id="UpdateOutSourceAdds" tabindex="-1" role="dialog" aria-labelledby="myModalLabel34" aria-modal="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel34">Edit Out Source Adds</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <form id="UpdateOutSourceAdds_Submit_Form">
                <input type="hidden" id="UOSAId">
                <div class="form-body">
                    <div class="card">
                        <div class="card-content collapse show">
                            <div class="card-body">
                                <div class="form-body">
                                    <fieldset>
                                        <div class="control-group row">
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Name:</label>
                                                    <input id="UOSAName" name="UOSAName" type="text" placeholder="Name" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Button Name:</label>
                                                    <input id="UOSABtnName" name="UOSABtnName" type="text" placeholder="Button Name" class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="control-group row">
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Website:</label>
                                                    <input id="UOSAWebsite" name="UOSAWebsite" type="text" placeholder="Website Name" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Description:</label>
                                                    <textarea id="UOSADescription" name="UOSADescription" type="text" placeholder="Description" class="form-control"></textarea>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="control-group row">
                                            <div class="col-md-12">
                                                <div class="form-group position-relative">
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="" id="UOSAIsActive">
                                                        <label class="form-check-label" for="UOSAIsActive">
                                                            IsActive
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="control-group row">
                                            <div class="col-md-12">
                                                <div class="form-group position-relative">
                                                    <img id="UOSAtargetBanner" class="border border-primary img-thumbnail">
                                                    <label>Banner Image<span class="text-danger"></span></label>
                                                    <input class="form-control upload" type="file" accept="image/*" id="UOSAimageBannerfiles">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="control-group row">
                                            <div class="col-md-12">
                                                <div class="form-group position-relative">
                                                    <img id="UOSAMobtargetBanner" class="border border-primary img-thumbnail">
                                                    <label>Mobile Banner Image<span class="text-danger"></span></label>
                                                    <input class="form-control upload" type="file" accept="image/*" id="UOSAMobimageBannerfiles">
                                                </div>
                                            </div>
                                        </div>

                                    </fieldset>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div id="UpdateOutSourceAddShowButtons" class="modal-footer">
                        <button type="reset" class="btn btn-danger" data-dismiss="modal">Cancel</button>
                        <button type="submit" class="btn btn-primary">Save</button>
                    </div>
                    <div id="UpdateOutSourceAddShowLoader" class="modal-footer" style="display:none">
                        <button class="btn btn-primary" type="button" disabled="">
                            <span class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true"></span>
                            Saving Results...
                        </button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>


