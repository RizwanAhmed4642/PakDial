<div class="modal fade text-left show" id="AddOutSourceAdds" tabindex="-1" role="dialog" aria-labelledby="myModalLabel34" aria-modal="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel34">Create Out Source Adds</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>

            <form id="AddOutSourceAdds_Submit_Form">
                <div class="form-body">
                    <div class="card">
                        <div class="card-content collapse show">
                            <div class="card-body">
                                <div class="form-body">
                                    <fieldset>
                                        <div class="control-group row">
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Name:</label>
                                                    <input id="AOSAName" name="AOSAName" type="text" placeholder="Name" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Button Name:</label>
                                                    <input id="AOSABtnName" name="AOSABtnName" type="text" placeholder="Button Name" class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="control-group row">
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Website:</label>
                                                    <input id="AOSAWebsite" name="AOSAWebsite" type="text" placeholder="Website Name" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Description</label>
                                                    <textarea id="AOSADescription" name="AOSADescription" type="text" placeholder="Description" class="form-control"></textarea>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="control-group row">
                                            <div class="col-md-12">
                                                <div class="form-group position-relative">
                                                    <img id="AOSAtargetBanner" class="border border-primary img-thumbnail">
                                                    <label>Banner Image<span class="text-danger"></span></label>
                                                    <input class="form-control upload" type="file" accept="image/*" id="AOSAimageBannerfiles">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="control-group row">
                                            <div class="col-md-12">
                                                <div class="form-group position-relative">
                                                    <img id="AOSAMobtargetBanner" class="border border-primary img-thumbnail">
                                                    <label>Mobile Banner Image<span class="text-danger"></span></label>
                                                    <input class="form-control upload" type="file" accept="image/*" id="AOSAMobimageBannerfiles">
                                                </div>
                                            </div>
                                        </div>

                                    </fieldset>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div id="AddOutSourceAddShowButtons" class="modal-footer">
                        <button type="reset" class="btn btn-danger" data-dismiss="modal">Cancel</button>
                        <button type="submit" class="btn btn-primary">Save</button>
                    </div>
                    <div id="AddOutSourceAddShowLoader" class="modal-footer" style="display:none">
                        <button class="btn btn-primary" type="button" disabled="">
                            <span class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true"></span>
                            Saving Results...
                        </button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
