<div class="card">
    <div class="card-header inner-card">
        <h4 class="card-title" id="basic-layout-form">Other Information</h4>
        <a class="heading-elements-toggle">
            <i class="la la-ellipsis-v font-medium-3"></i>
        </a>
        <div class="heading-elements">
            <ul class="list-inline mb-0">
                <li>
                    <a href="#UACOtherInfo" data-toggle="collapse" data-target="#UACOtherInfo" aria-expanded="false" aria-controls="UACOtherInfo">
                    </a>
                </li>


            </ul>
        </div>

    </div>
    <div class="card-content collapse" id="UACOtherInfo" data-parent="#EditListingData" >
        <div class="card-body">
            @await Html.PartialAsync("~/Views/AdminCompanyListings/_EditCompanyTimingView.cshtml")
            <hr />
            <!--Company Information-->
            <hr />
            <h4 class="card-title" id="file-repeater">Company Information</h4>
            <div class="form-body">
                <input type="hidden" id="UCompProfileId" />
                <div class="form row">
                    <div class="form-group col-md-3">
                        <label>Year Of Establishment :</label>
                    </div>
                    <div class="form-group col-md-3">
                        <input type="Text" class="form-control" id="UCLstYears" placeholder="1995">
                    </div>
                    <div class="form-group col-md-3">
                        <input type="Text" class="form-control" id="UCLstAnnualTurnover" placeholder="Annual Turnover">
                    </div>
                    <div class="form-group col-md-3">
                        <select class="form-control" name="no_of_employee" id="UCLstNoEmployee">
                            <option value="">Select No of Employees</option>
                            <option value="Less than 10">Less than 10</option>
                            <option value="10-100">10-100</option>
                            <option value="100-500">100-500</option>
                            <option value="500-1000">500-1000</option>
                            <option value="1000-2000">1000-2000</option>
                            <option value="2000-5000">2000-5000</option>
                            <option value="5000-10000">5000-10000</option>
                            <option value="More than 10000">More than 10000</option>
                        </select>
                    </div>
                </div>
                <div class="form row">
                    <div class="form-group col-md-3">
                        <label>Professional Associations :</label>
                    </div>
                    <div class="form-group col-md-9">
                        <input type="Text" class="form-control" id="UCLstProfAss" placeholder="Professional Associations">
                    </div>
                </div>
                <div class="form row">
                    <div class="form-group col-md-3">
                        <label>Certifications  :</label>
                    </div>
                    <div class="form-group col-md-9">
                        <input type="Text" class="form-control" id="UCLstCertifications" placeholder="Certifications ">
                    </div>
                </div>

                <div class="form row">
                    <div class="form-group col-md-3">
                        <label>Brief About  :</label>
                    </div>
                    <div class="form-group col-md-9">
                        <textarea type="Text" class="form-control" id="UCLstBreifAbout" placeholder="Brief About "></textarea>
                    </div>
                </div>
                <div class="form row">
                    <div class="form-group col-md-3">
                        <label>Product And Services  :</label>
                    </div>
                    <div class="form-group col-md-9">
                        <textarea type="Text" class="form-control" id="UCLstProductAndServices" placeholder="Product And Services"></textarea>
                    </div>
                </div>
                <div class="form row">
                    <div class="form-group col-md-3">
                        <label>Location and Overview  :</label>
                    </div>
                    <div class="form-group col-md-9">
                        <textarea type="Text" class="form-control" id="UCLstLocationOverview" placeholder="Location and Overview  "></textarea>
                    </div>
                </div>

                <!-- Payment Modes and Products and Services offered-->
                <hr />
                <h4 class="card-title" id="file-repeater">Payment Modes and Services offered</h4>
                <div class="control-group row">

                    <div class="col-md-6">
                        <label>Payment Modes</label>
                        <div class="form-group position-relative">
                            <select id="UCLstPaymentsModes" name="UCLstPaymentsModes" class="form-control" style="width:100%" data-toggle="tooltip" data-trigger="hover" data-placement="top" multiple="multiple"></select>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <label>Services Type</label>
                        <div class="form-group position-relative">
                            <select id="UCLstServicetypes" name="UCLstServicetypes" class="form-control" style="width:100%" data-toggle="tooltip" data-trigger="hover" data-placement="top" multiple="multiple"></select>
                        </div>
                    </div>
                </div>
                <hr />
                <!--bussiness TypesMode-->
                <h4 class="card-title" id="file-repeater">Business Types</h4>
                <div class="control-group row">

                    <div class="col-md-6">
                        <label>Business Types</label>
                        <div class="form-group position-relative">
                            <select id="UCLstBusinessTypes" name="UCLstBusinessTypes" class="form-control" style="width:100%" data-toggle="tooltip" data-trigger="hover" data-placement="top" multiple="multiple"></select>
                        </div>
                    </div>

                </div>
                <!--Social Media-->
                <hr />
                <h4 class="card-title" id="file-repeater">Social Media Links</h4>
                <div class="form-body" id="UAddSocialMediaModesIds">
                    <!--Adding Social Media Modes Using Jquery-->
                </div>
            </div>
        </div>
    </div>
</div>