<div class="modal fade text-left show" id="UpdateTransferOwnerShip" role="dialog" aria-labelledby="myModalLabel34" aria-modal="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel34">Ownership Transfer</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <form id="UpdateTransferOwnership_Submit_Form">
                <input type="hidden" id="UTransferListingsId" />
                <div class="form-body">
                    <div class="card">
                        <div class="card-content collapse show">
                            <div class="card-body">
                                <div class="form-body">
                                    <fieldset>
                                        <div class="control-group row">
                                            <div class="col-md-12">
                                                <div class="form-group position-relative">
                                                    <label>Customer</label>
                                                    <select id="UTransferCutIdId" name="UTransferCutIdId" class="select2 form-control" style="width:100%" data-toggle="tooltip" data-trigger="hover" data-placement="top"></select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="control-group row">
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Company Name</label>
                                                    <input id="UTransferCompanyName" name="UTransferCompanyName" readonly type="text" placeholder="Company Name" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Name</label>
                                                    <input id="UTransferFullName" name="UTransferFullName" readonly type="text" placeholder="Name" class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div id="UpdateTransferOwnershipShowButtons" class="modal-footer pr-2 pl-2">
                        <button type="reset" class="btn btn-danger" data-dismiss="modal">Cancel</button>
                        <button type="submit" class="btn btn-primary">Save</button>
                    </div>
                    <div id="UpdateTransferOwnershipShowLoader" class="modal-footer pr-2 pl-2" style="display:none">
                        <button class="btn btn-primary" type="button" disabled="">
                            <span class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true"></span>
                            Transferring...
                        </button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
