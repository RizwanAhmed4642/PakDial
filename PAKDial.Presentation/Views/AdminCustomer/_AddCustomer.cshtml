<div class="modal fade text-left show" id="AddCustomer" tabindex="-1" role="dialog" aria-labelledby="myModalLabel34" aria-modal="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel34">Create Cutomer</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <form id="AddCustomer_Submit_Form">
                <div class="form-body">
                    <div class="card">
                        <div class="card-content collapse show">
                            <div class="card-body">
                                <div class="form-body">
                                    <fieldset class="scheduler-border">
                                        <legend class="scheduler-border"><h4 class="card-title">Profile Image</h4></legend>
                                        <div class="control-group row">
                                            <div class="col-12 d-flex justify-content-center">
                                                <div class="w-25">
                                                    <img class="border border-primary w-100" id="AtargetImg">
                                                    <label class="btn btn-primary w-100 mt-1" for="Aimagefiles">Thumbnail</label>
                                                    <input type="file" accept="image/*" class="upload" style="display:none" id="Aimagefiles">
                                                </div>
                                            </div>
                                        </div>

                                    </fieldset>
                                    <fieldset class="scheduler-border">
                                        <legend class="scheduler-border"><h4 class="card-title">Personal Information</h4></legend>
                                        <div class="control-group row">
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>First Name<span class="text-danger">*</span></label>
                                                    <input id="ACustFirstName" name="ACustFirstName" type="text" placeholder="First Name" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Last Name<span class="text-danger">*</span></label>
                                                    <input id="ACustLastName" name="ACustLastName" type="text" placeholder="Last Name" class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="control-group row">
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Date Of Birth<span class="text-danger">*</span></label>
                                                    <input id="ACustDob" name="ACustDob" type="date" placeholder="DOB" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <div class="form-group position-relative">
                                                        <label>CNIC<span class="text-danger">*</span></label>
                                                        <input id="ACustCnic" name="ACustCnic" type="text" placeholder="00000-0000000-0" class="form-control" maxlength="15">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </fieldset>
                                    <fieldset class="scheduler-border">
                                        <legend class="scheduler-border"><h4 class="card-title">Contact Information</h4></legend>
                                        <div class="control-group row">

                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Contact Number</label>
                                                    <input id="ACustContactNumber" name="ACustContactNumber" type="text" placeholder="042 12345678" class="form-control" maxlength="11">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <div class="position-relative ">
                                                        <label>Phone Number</label>
                                                        <input type="text" id="ACustPhoneNumber" name="ACustPhoneNumber" class="form-control" placeholder="+92 000 0000000" maxlength="13">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                    <fieldset class="scheduler-border">
                                        <legend class="scheduler-border"><h4 class="card-title">Credentials Information</h4></legend>

                                        <div class="control-group row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <div class="position-relative ">
                                                        <label>Role</label>
                                                        <select id="ACustRole" name="ACustRole" class="form-control" data-toggle="tooltip" data-trigger="hover" data-placement="top"></select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="control-group row">
                                            <div class="col-md-6">
                                                <div class="form-group position-relative">
                                                    <label>Email<span class="text-danger">*</span></label>
                                                    <input id="ACustEmail" name="ACustEmail" type="text" placeholder="Email" class="form-control">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <div class="position-relative ">
                                                        <label>Password<span class="text-danger">*</span></label>
                                                        <input id="ACustPassword" name="ACustPassword" style="-webkit-text-security: disc;" placeholder="Password" class="form-control" autocomplete="off">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </fieldset>

                                </div>
                                <div id="AddCustShowButtons" class="modal-footer employeemodalfooter">
                                    <button type="reset" class="btn btn-danger" data-dismiss="modal">Cancel</button>
                                    <button type="submit" class="btn btn-primary">Save</button>
                                </div>
                                <div id="AddCustShowLoader" class="modal-footer employeemodalfooter" style="display:none">
                                    <button class="btn btn-primary" type="button" disabled="">
                                        <span class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true"></span>
                                        Results Saving...
                                    </button>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>
