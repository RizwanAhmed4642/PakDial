@model IEnumerable<PAKDial.Domains.UserEndViewModel.CompanyListingSubCate>
@if (Model.Count() > 0)
{
    <input type="hidden" id="SbCId1" value="@Model.FirstOrDefault().MainCategoryId" />
    <input type="hidden" id="SbCName1" value="@Model.FirstOrDefault().MainCategoryName" />
}
@foreach (var item in Model)
{
    <li class="">
        <a asp-controller="Home"
           asp-action="LoadCategoryDescription"
           asp-route-CtName="@item.Location" asp-route-SbCId="@item.Id"
           asp-route-SbCName="@item.Name.ToString().Replace(" ","_")" asp-route-SortFilter="topResults"
           asp-route-Ratingstatus="Asc"
           class="listed-cat">@item.Name</a>
    </li>
}


