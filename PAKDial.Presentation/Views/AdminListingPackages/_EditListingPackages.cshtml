<div class="modal fade text-left show" id="UpdateListingPackages" tabindex="-1" role="dialog" aria-labelledby="myModalLabel34" aria-modal="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel34">Edit Listing Packages</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <form id="UpdateListingPackages_Submit_Form">
                <div class="modal-body">
                    <input type="hidden" id="UListingPackagesId" />
                    <input type="hidden" id="UPackagesPriceId" />
                    <label>Name: </label>
                    <div class="form-group position-relative">
                        <input id="UListingPackagesName" name="UListingPackagesName" type="text" placeholder="Name" class="form-control">
                    </div>

                    <label>Description: </label>
                    <div class="form-group position-relative">
                        <textarea id="UListingPackagesDescrption" name="UListingPackagesDescrption" type="text" placeholder="Description" class="form-control"></textarea>
                    </div>

                    <label>Months: </label>
                    <div class="form-group position-relative">
                        <input id="UListingPackagesMonths" name="UListingPackagesMonths" type="number" placeholder="Months" class="form-control">
                    </div>

                    <label>Price: </label>
                    <div class="form-group position-relative">
                        <input readonly id="UListingPackagesPrice" name="UListingPackagesPrice" type="text" placeholder="Price" class="form-control">
                    </div>

                    <label>New Price: </label>
                    <div class="form-group position-relative">
                        <input id="UListingPackagesNewPrice" name="UListingPackagesNewPrice" type="text" placeholder="New Price" class="form-control">
                    </div>

                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" value="" id="UListingPackagesIsActive">
                        <label class="form-check-label" for="UListingPackagesIsActive">
                            IsActive
                        </label>
                    </div>

                </div>
                <div id="UpdateListingPackagesShowButtons" class="modal-footer">
                    <button type="reset" class="btn btn-danger" data-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn btn-primary">Save</button>
                </div>
                <div id="UpdateListingPackagesShowLoader" class="modal-footer" style="display:none">
                    <button class="btn btn-primary" type="button" disabled="">
                        <span class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true"></span>
                        Results Saving...
                    </button>
                </div>
            </form>
        </div>
    </div>
</div>

