<div class="modal fade text-left show" id="AddRoute" tabindex="-1" role="dialog" aria-labelledby="myModalLabel34" aria-modal="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel34">Create Routes</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <form id="AddRoute_Submit_Form">
                <div class="modal-body">
                    
                    <label>Modules: </label>
                    <div class="form-group position-relative">
                        <select id="AModuleId" name="AModuleId" class="form-control" data-toggle="tooltip" data-trigger="hover" data-placement="top"></select>
                    </div>

                    <label>MenuName: </label>
                    <div class="form-group position-relative">
                        <input id="ARouteMenuName" name="ARouteMenuName" type="text" placeholder="Menu Display Name" class="form-control">
                    </div>

                    <label>Description: </label>
                    <div class="form-group position-relative">
                        <input id="ARouteDescription" name="ARouteDescription" type="text" placeholder="Description" class="form-control">
                    </div>

                    <label>Controller: </label>
                    <div class="form-group position-relative">
                        <input id="ARouteController" name="ARouteController" type="text" placeholder="Controller Name" class="form-control">
                    </div>

                    <label>Action: </label>
                    <div class="form-group position-relative">
                        <input id="ARouteAction" name="ARouteAction" type="text" placeholder="Action Name" class="form-control">
                    </div>

                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" value="" id="ARouteStatus">
                        <label class="form-check-label" for="ARouteStatus">
                            IsActive
                        </label>
                    </div>

                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" value="" id="ARouteIsShown">
                        <label class="form-check-label" for="ARouteIsShown">
                            IsShown
                        </label>
                    </div>

                </div>
                <div id="AddRouteShowButtons" class="modal-footer">
                    <button type="reset" class="btn btn-danger" data-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn btn-primary">Save</button>
                </div>
                <div id="AddRouteShowLoader" class="modal-footer" style="display:none">
                    <button class="btn btn-primary" type="button" disabled="">
                        <span class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true"></span>
                        Results Saving...
                    </button>
                </div>
            </form>
        </div>
    </div>
</div>


