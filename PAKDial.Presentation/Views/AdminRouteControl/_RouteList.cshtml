                                        <th scope="col">Actions</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    <tr>
                                        <th scope="col">Sr #</th>
                                        <th scope="col">Controller</th>
                                        <th scope="col">Action</th>
                                        <th scope="col">IsActive</th>
                                        <th scope="col">Module</th>
